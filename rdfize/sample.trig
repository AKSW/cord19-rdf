            <http://www.example.org/refSpan>
                    <http://covid19.aksw.org/text-6a8360e72ec32e350a9696c542978e6c95f72cfb4ec871fd26fe7a00e0acc7d3_refspan-1977-1985> , <http://covid19.aksw.org/text-6a8360e72ec32e350a9696c542978e6c95f72cfb4ec871fd26fe7a00e0acc7d3_refspan-1445-1453> , <http://covid19.aksw.org/text-6a8360e72ec32e350a9696c542978e6c95f72cfb4ec871fd26fe7a00e0acc7d3_refspan-648-656> , <http://covid19.aksw.org/text-6a8360e72ec32e350a9696c542978e6c95f72cfb4ec871fd26fe7a00e0acc7d3_refspan-2584-2592> ;
