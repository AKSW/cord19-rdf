            <http://www.example.org/bodyJson>
                    "{\"text\":\"Markedly enlarged and congested.\",\"cite_spans\":[],\"ref_spans\":[],\"section\":\"Spleen\"}"^^<http://www.w3.org/2001/XMLSchema#json> ;
            <http://www.example.org/section>
