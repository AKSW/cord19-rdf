            <http://www.example.org/refSpan>
                    <http://covid19.aksw.org/text-faf4c0d39849b42d6917ce3306b75a7e190bd5f44cda79c7006dd0c1e85ef9ca_refspan-717-726> , <http://covid19.aksw.org/text-faf4c0d39849b42d6917ce3306b75a7e190bd5f44cda79c7006dd0c1e85ef9ca_refspan-240-249> , <http://covid19.aksw.org/text-faf4c0d39849b42d6917ce3306b75a7e190bd5f44cda79c7006dd0c1e85ef9ca_refspan-1035-1044> , <http://covid19.aksw.org/text-faf4c0d39849b42d6917ce3306b75a7e190bd5f44cda79c7006dd0c1e85ef9ca_refspan-691-699> , <http://covid19.aksw.org/text-faf4c0d39849b42d6917ce3306b75a7e190bd5f44cda79c7006dd0c1e85ef9ca_refspan-532-540> ;
